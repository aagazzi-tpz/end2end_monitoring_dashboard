{
    "collectors": [
        {
            "class": "ODataCollectorConfiguration",
            "id_field": [
                "name",
                "timestamp",
                "interface_name"
            ],
            "date_attr": "timestamp",
            "routing_key": "new.raw.data.metrics-product",
            "interface_name": "metrics_LTA_S5P_DLR",
            "file_pattern": "metrics_LTA_S5P_DLR_*.json",
            "refresh_interval": 10,
            "expected_collect_interval": 2160,
            "product_per_page": 1000,
            "odata_entities": "metrics.json",
            "odata_entity_location": "/",
            "odata_query_filter": "startswith(Name, 'Archived') and Timestamp ge {publication_start_date} and Timestamp lt {publication_end_date}",
            "odata_query_order_by": "Timestamp",
            "disable_insecure_request_warning": true,
            "model": "MetricsProduct",
            "no_probe": true,
            "extractor": {
                "class": "JSONExtractor",
                "args": {
                    "attr_map": {
                        "name": "`this`.Name",
                        "timestamp": "`this`.Timestamp",
                        "metric_type": "`this`.MetricType",
                        "counter": "`this`.Counter",
                        "interface_name": {
                            "python": "lambda c: 'metrics_LTA_S5P_DLR'"
                        },
                        "production_service_type": {
                            "python": "lambda c: 'LTA'"
                        },
                        "production_service_name": {
                            "python": "lambda c: 'S5P_DLR'"
                        }
                    },
                    "iterate_nodes": "$.value",
                    "allow_partial": true
                }
            }
        }
    ]
}
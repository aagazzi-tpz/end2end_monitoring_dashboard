{
    "aliases": {
        "raw-data-prip-product": {}
    },
    "index_patterns": [
        "raw-data-prip-product-*"
    ],
    "settings": {
        "index": {
            "number_of_shards": 3,
            "number_of_replicas": 1
        }
    },
    "mappings": {
        "properties": {
            "ingestionTime": {
                "type": "date",
                "format": "date_time",
                "locale": "utc"
            },
            "reportName": {
                "type": "keyword"
            },
            "reportFolder": {
                "type": "keyword"
            },
            "product_id": {
                "type": "keyword"
            },
            "product_name": {
                "type": "keyword"
            },
            "content_length": {
                "type": "long"
            },
            "publication_date": {
                "type": "date",
                "locale": "utc"
            },
            "start_date": {
                "type": "date",
                "locale": "utc"
            },
            "end_date": {
                "type": "date",
                "locale": "utc"
            },
            "interface_name": {
                "type": "keyword"
            },
            "production_service_type": {
                "type": "keyword"
            },
            "production_service_name": {
                "type": "keyword"
            },
            "eviction_date": {
                "type": "date",
                "locale": "utc"
            },
            "origin_date": {
                "type": "date",
                "locale": "utc"
            },
            "footprint": {
                "type": "geo_shape",
                "coerce": true,
                "ignore_malformed": true
            },
            "product_group_id": {
                "type": "keyword"
            },
            "datastrip_id": {
                "type": "keyword"
            },
            "quality_status": {
                "type": "keyword"
            },
            "cloud_cover": {
                "type": "float"
            }
        },
        "_meta": {
            "partition_field": "ingestionTime",
            "partition_format": "%Y-%m"
        }
    }
}
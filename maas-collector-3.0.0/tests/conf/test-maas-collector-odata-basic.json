{
    "collectors": [
        {
            "class": "ODataCollectorConfiguration",
            "id_field": "product_id",
            "routing_key": "new.raw.data.basic-product",
            "interface_name": "basic_test",
            "hours_delta": 1,
            "product_per_page": 1000,
            "model": {
                "index": "raw-data-basic-product",
                "name": "TestProduct",
                "fields": [
                    {
                        "name": "product_id",
                        "type": "Keyword"
                    },
                    {
                        "name": "product_name",
                        "type": "Keyword"
                    },
                    {
                        "name": "content_length",
                        "type": "Integer"
                    },
                    {
                        "name": "publication_date",
                        "type": "Date"
                    }
                ]
            },
            "extractor": {
                "class": "JSONExtractor",
                "args": {
                    "attr_map": {
                        "product_id": "`this`.Id",
                        "product_name": "`this`.Name",
                        "content_length": "`this`.ContentLength",
                        "publication_date": "`this`.PublicationDate"
                    },
                    "iterate_nodes": "$.value"
                }
            }
        }
    ]
}